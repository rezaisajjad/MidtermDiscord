<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.graphiscord.SettingViewController">
   <children>
      <Circle fx:id="profilePic" fill="DODGERBLUE" layoutX="120.0" layoutY="99.0" radius="31.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
      <Button layoutX="550.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitSettings" text="ESC" />
      <Label fx:id="userName" layoutX="171.0" layoutY="87.0" prefHeight="25.0" prefWidth="115.0" text="Label" />
      <Button layoutX="391.0" layoutY="87.0" mnemonicParsing="false" onAction="#changePicButton" text="Change Profile Picture" />
      <Rectangle fx:id="state" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="60.0" layoutX="84.0" layoutY="70.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="5.0" />
      <TextField fx:id="usernameTextField" editable="false" layoutX="84.0" layoutY="188.0" />
      <TextField fx:id="passwordTextField" editable="false" layoutX="84.0" layoutY="322.0" />
      <TextField fx:id="emailTextField" editable="false" layoutX="84.0" layoutY="255.0" />
      <Label layoutX="89.0" layoutY="163.0" prefHeight="17.0" prefWidth="126.0" text="Username" />
      <Label layoutX="88.0" layoutY="296.0" prefHeight="17.0" prefWidth="126.0" text="Password" />
      <Label layoutX="88.0" layoutY="231.0" prefHeight="17.0" prefWidth="126.0" text="Email" />
      <Label fx:id="usernameValidity" layoutX="258.0" layoutY="192.0" text="Label" />
      <Label fx:id="passwordValidity" layoutX="258.0" layoutY="326.0" text="Label" />
      <Label fx:id="emailValidity" layoutX="258.0" layoutY="259.0" text="Label" />
      <Button layoutX="488.0" layoutY="188.0" mnemonicParsing="false" onAction="#usernameEditButton" text="Edit" />
      <Button layoutX="488.0" layoutY="322.0" mnemonicParsing="false" onAction="#passwordEditButton" text="Edit" />
      <Button layoutX="488.0" layoutY="255.0" mnemonicParsing="false" onAction="#emailEditButton" text="Edit" />
   </children>
</AnchorPane>
