    <Label fx:id="phoneValidity" layoutX="254.0" layoutY="365.0" prefHeight="17.0" prefWidth="175.0" text="Label" textFill="RED" visible="false" />
    <Button layoutX="484.0" layoutY="361.0" mnemonicParsing="false" onAction="#phoneEditButton" text="Edit" />
    <Button layoutX="490.0" layoutY="58.0" mnemonicParsing="false" onAction="#logoutButton" prefHeight="26.0" prefWidth="89.0" text="Log out" textFill="RED">
        <font>
            <Font size="14.0" />
        </font></Button>
    <ComboBox fx:id="statusComboBox" layoutX="139.0" layoutY="26.0" onAction="#changeStatus" prefHeight="26.0" prefWidth="142.0">
    </ComboBox>
</AnchorPane>
