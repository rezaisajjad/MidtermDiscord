    <Circle fx:id="profilePic1" fill="#bad4ec" layoutX="70.0" layoutY="70.0" radius="56.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
    <Circle fx:id="profilePic"  onMouseClicked="#changePicButton" fill="DODGERBLUE" layoutX="70.0" layoutY="70.0" radius="50.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
    <Button layoutX="489.0" layoutY="13.0" mnemonicParsing="false" onAction="#exitSettings" prefHeight="26.0" prefWidth="90.0" text="Back">
        <font>
            <Font size="15.0" />
        </font></Button>
    <Label fx:id="userName" layoutX="140.0" layoutY="71.0" prefHeight="26.0" prefWidth="142.0" text="Label" />
    <TextField fx:id="usernameTextField" layoutX="80.0" layoutY="159.0" />
    <TextField fx:id="passwordTextField" layoutX="80.0" layoutY="293.0" />
    <TextField fx:id="emailTextField" layoutX="80.0" layoutY="226.0" />
    <Label layoutX="85.0" layoutY="134.0" prefHeight="17.0" prefWidth="126.0" text="Username" />
    <Label layoutX="84.0" layoutY="267.0" prefHeight="17.0" prefWidth="126.0" text="Password" />
    <Label layoutX="84.0" layoutY="202.0" prefHeight="17.0" prefWidth="126.0" text="Email" />
    <Label fx:id="usernameValidity" layoutX="254.0" layoutY="163.0" prefHeight="17.0" prefWidth="175.0" text="Label" textFill="RED" visible="false" />
    <Label fx:id="passwordValidity" layoutX="254.0" layoutY="297.0" prefHeight="17.0" prefWidth="175.0" text="Label" textFill="RED" visible="false" />
    <Label fx:id="emailValidity" layoutX="254.0" layoutY="230.0" prefHeight="17.0" prefWidth="175.0" text="Label" textFill="RED" visible="false" />
    <Button layoutX="484.0" layoutY="159.0" mnemonicParsing="false" onAction="#usernameEditButton" text="Edit" />
    <Button layoutX="484.0" layoutY="293.0" mnemonicParsing="false" onAction="#passwordEditButton" text="Edit" />
    <Button layoutX="484.0" layoutY="226.0" mnemonicParsing="false" onAction="#emailEditButton" text="Edit" />
    <TextField fx:id="phoneTextField" layoutX="80.0" layoutY="361.0" />
